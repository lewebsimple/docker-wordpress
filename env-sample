# Docker Compose project name
COMPOSE_PROJECT_NAME=example

# Default TLD
TLD=example.com

# Default data root directory
DATA=./data

# Let's encrypt
LETSENCRYPT_EMAIL=you@example.com

# Apache2
APACHE2_CONTAINER=apache2

# Enable Xdebug
XDEBUG_ENABLE=yes

# MySQL
MYSQL_HOST=mysql
MYSQL_ROOT_PASSWORD=changeme

# SMTP relay
SMTP_RELAY=maildev
